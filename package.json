{
    "name": "@fleetbase/storefront-app",
    "version": "0.0.16",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "web": "TAMAGUI_TARGET=web webpack serve --config webpack.config.js && ./node_modules/.bin/postcss public/tamagui.css -o public/tamagui.css",
        "pod:install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update",
        "pod:reset": "rm -rf ios/Pods/ ios/Podfile.lock && cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update",
        "pod:repo-update": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod repo update",
        "pod:deintegrate": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod deintegrate && yarn run pod:install",
        "reset": "rm -rf node_modules/ yarn.lock && yarn install && yarn run pod:reset",
        "test": "jest",
        "lint": "eslint .",
        "format": "prettier --write .",
        "tamagui:build": "./node_modules/.bin/tamagui-build",
        "tamagui:clean": "./node_modules/.bin/tamagui-build clean",
        "android:clean": "cd android && ./gradlew clean && cd ../",
        "adb:reverse": "adb reverse tcp:8081 tcp:8081",
        "adb:devmenu": "adb shell input keyevent 82",
        "adb:reload": "adb shell input text 'RR'",
        "adb:restart": "adb shell am broadcast -a react.native.RELOAD",
        "adb:open-settings": "adb shell am start -a android.settings.SETTINGS",
        "adb:logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
        "adb:logcat:errors": "adb logcat *:E",
        "adb:logcat:grep": "adb logcat | grep 'ReactNativeJS'",
        "adb:devices": "adb devices",
        "generate:app-icon": "npx app-icon generate -i ./assets/app-icon.png",
        "generate:launch-screen": "npx react-native generate-bootsplash ./assets/splash-screen.png --background=#FFFFFF --logo-width=100 --flavor=main",
        "postinstall": "patch-package"
    },
